{
  "name": "learning-plattform",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "e2e:ci": "npx playwright test",
    "e2e:ui": "npx playwright test --ui",
    "preview": "vite preview",
    "test:unit": "vitest --coverage",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "npx eslint ./src/**/*.ts ./src/**/*.vue",
    "lint:fix": "npx eslint --fix ./src/**/*.ts ./src/**/*.vue",
    "generate:api": "npx swagger-typescript-api -p ./../fox-deck-api/docs/swagger.json -o ./src/core/services/ -n api.ts --axios",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@material/web": "^1.1.1",
    "@vee-validate/i18n": "^4.11.8",
    "@vee-validate/rules": "^4.11.8",
    "@vueuse/core": "^10.6.1",
    "axios": "^1.6.0",
    "feather-icons": "^4.29.1",
    "pinia": "^2.1.7",
    "qs": "^6.11.2",
    "vee-validate": "^4.11.8",
    "vite-svg-loader": "^4.0.0",
    "vue": "^3.3.7",
    "vue-feather": "^2.0.0",
    "vue-i18n": "^9.7.1",
    "vue-inline-svg": "^2.1.3",
    "vue-jwt-decode": "^0.1.0",
    "vue-router": "^4.2.5",
    "@vue/eslint-config-typescript": "^12.0.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.4.0",
    "@playwright/test": "^1.40.0",
    "@storybook/addon-essentials": "^8.1.3",
    "@storybook/addon-interactions": "^8.1.3",
    "@storybook/addon-links": "^8.1.3",
    "@storybook/blocks": "^8.1.3",
    "@storybook/test": "^8.1.3",
    "@storybook/vue3": "^8.1.3",
    "@storybook/vue3-vite": "^8.1.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^18.18.5",
    "@types/qs": "^6.9.9",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitest/coverage-istanbul": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "@vue/test-utils": "^2.4.2",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.16",
    "jsdom": "^22.1.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.69.5",
    "storybook": "^8.1.3",
    "swagger-typescript-api": "^13.0.3",
    "tailwindcss": "^3.3.5",
    "typescript": "~5.2.0",
    "vite": "^4.4.11",
    "vitest": "^0.34.6",
    "vitest-fetch-mock": "^0.2.2",
    "vue-tsc": "^1.8.19",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.57.0",
    "eslint-import-resolver-alias": "^1.1.2",
